<?xml version='1.0' encoding='UTF-8' ?>
<odoo>
    <data>
        <!--list view -->
        <record id="contact_ikama_tree" model="ir.ui.view">
            <field name="name">Contact Ikama Tree</field>
            <field name="model">ebs_mod.contact.ikama</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="family_member"/>
                    <field name="dependent"/>
                    <field name="ikama_no" />
                    <field name="ikama_enddate"/>
                    <field name="guarantor"/>
                    <field name="occupation"/>

                    <field name="is_active" />
                </tree>
            </field>
        </record>

        <record id="contact_ikama_form" model="ir.ui.view">
            <field name="name">Contact QID Form</field>
            <field name="model">ebs_mod.contact.ikama</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group name="group_top">
                            <group name="group_left">
                                <field name="family_member"/>
                                <field name="dependent"/>
                                <field name="ikama_no"/>
                                <field name="ikama_enddate"/>
                                <field name="guarantor"/>
                                <field name="occupation"/>
                                <field name="is_active" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>