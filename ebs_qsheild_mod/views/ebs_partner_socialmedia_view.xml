<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>

        <record id="ebs_socialmedia_types_form_view" model="ir.ui.view">
            <field name="name">ebs.socialmedia.types.form</field>
            <field name="model">ebs.socialmedia.types</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="ebs_socialmedia_types_tree_view" model="ir.ui.view">
            <field name="name">ebs.socialmedia.types.tree</field>
            <field name="model">ebs.socialmedia.types</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>


        <record id="ebs_socialmedia_types_act_window" model="ir.actions.act_window">
            <field name="name">Social Media Types</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ebs.socialmedia.types</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Social Media Types" action="ebs_socialmedia_types_act_window" id="ebs_socialmedia_types_menu"
                  parent="ebs_qsheild_mod.menu_localisation"/>

    </data>

</odoo>